{
  "name": "Override Fonts",
  "description": "Overrides fonts",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.github.com/*"],
      "css" : ["github_custom_font.css"]
    },
    {
      "matches": ["https://*.airflow.amazonaws.com/*/*"],
      "css" : ["airflow_custom_font.css"]
    },
    {
      "matches": ["<all_urls>"],
      "css" : ["custom_font.css"],
      "exclude_globs": [
        "https://*.github.com/*",
        "https://*.airflow.amazonaws.com/*"
      ]
    }
  ]
}